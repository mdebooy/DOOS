<persistence version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                                 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="doos" transaction-type="JTA">
    <jta-data-source>jdbc/DOOS</jta-data-source>

    <class>eu.debooy.doos.domain.I18nCodeDto</class>
    <class>eu.debooy.doos.domain.I18nCodeTekstDto</class>
    <class>eu.debooy.doos.domain.LijstDto</class>
    <class>eu.debooy.doos.domain.ParameterDto</class>
    <class>eu.debooy.doos.domain.TaalDto</class>
  </persistence-unit>
</persistence>
